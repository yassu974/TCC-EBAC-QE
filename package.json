{
  "name": "cy-intercept",
  "version": "1.0.0",
  "description": "Execicio Intercept do Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --config-file UI/cypress.config.js",
    "cy:run": "cypress run --config-file UI/cypress.config.js",
    "cy:run:chrome": "cypress run --browser chrome --config-file UI/cypress.config.js",
    "report:clean": "rimraf UI/cypress/reports",
    "report:open": "start \"\" \"UI/cypress/reports/index.html\"",
    "test:report": "npm run report:clean && npm run cy:run && npm run report:open"
  },
  "author": "renanferreira",
  "license": "ISC",
  "dependencies": {
    "cypress": "^14.5.4"
  },
  "devDependencies": {
    "cypress-mochawesome-reporter": "^4.0.2",
    "rimraf": "^6.1.3"
  }
}
